import { default as axios } from 'axios';
import HttpClient from './abstractHttpClient.js';


class DefaultHttpClient extends HttpClient{

    constructor(options) {
        super();
        this._axiosInstance = axios.create(options);

        this._axiosInstance.interceptors.request.use((config) => {
            return config;
        });
    }
    request(options) {
        options.httpsAgent = this._axiosInstance.httpsAgent
        return this._axiosInstance.request(options);
    }
}

export default DefaultHttpClient;